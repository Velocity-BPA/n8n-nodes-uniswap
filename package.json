{
  "name": "n8n-nodes-uniswap",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Uniswap DEX providing 22 resources and 200+ operations for swaps, liquidity, positions, quotes, routing, and analytics across V2, V3, and Universal Router.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-uniswap.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-uniswap",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-uniswap/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "uniswap",
    "defi",
    "dex",
    "swap",
    "ethereum",
    "amm",
    "liquidity",
    "blockchain",
    "web3",
    "trading",
    "concentrated-liquidity",
    "universal-router",
    "permit2"
  ],
  "main": "dist/nodes/Uniswap/Uniswap.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/UniswapNetwork.credentials.js",
      "dist/credentials/UniswapApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Uniswap/Uniswap.node.js",
      "dist/nodes/Uniswap/UniswapTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc --skipLibCheck || true && gulp build:icons",
    "build:strict": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist/",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@uniswap/permit2-sdk": "^1.3.0",
    "@uniswap/sdk-core": "^5.3.1",
    "@uniswap/smart-order-router": "^3.34.0",
    "@uniswap/universal-router-sdk": "^2.2.0",
    "@uniswap/v2-sdk": "^4.4.1",
    "@uniswap/v3-sdk": "^3.13.1",
    "axios": "^1.7.7",
    "ethers": "^6.13.2",
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "jsbi": "^3.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.5",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.2",
    "gulp": "^5.0.0",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.56.2",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
